			position: relative;
		}
		.m-menu a span {
			width: 100%;
			height: 3px;
			background: #000;
 			display: block;
			position: absolute;
			transition: all 0.3s;
		}
		.m-menu a span:nth-child(1) {
			top: 0px;
		}
		.m-menu a span:nth-child(2) {
			top: 8px;
		}
		.m-menu a span:nth-child(3) {
			bottom: 0px;
		}

		.m-menu a.active span:nth-child(1) {
/* 			transform: rotate(-45deg) translateY(12px); */
			-webkit-transform : translateY(12px) rotate(-315deg);
			transform : translateY(12px) rotate(-315deg);
			left: -1px;
		}
		.m-menu a.active span:nth-child(2) {
/* 			opacity: 0; */
			opacity : 0;
		}
		.m-menu a.active span:nth-child(3) {
/* 			transform: rotate(45deg) translateY(-5px); */
